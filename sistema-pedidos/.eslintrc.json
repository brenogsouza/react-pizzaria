{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "import/no-unresolved": [
      1,
      {
        "commonjs": true,
        "amd": true
      }
    ],
    "import/named": "off",
    "import/namespace": "off",
    "import/default": "off",
    "import/export": "off",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "react/prop-types": "off"
  }
}
